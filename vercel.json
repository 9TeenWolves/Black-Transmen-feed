{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/did.json",
      "dest": "/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ],
  {
  "rewrites": [
    { "source": "/.well-known/did.json", "destination": "/api/well-known-did" }
  ]
}

}
